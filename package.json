{
  "name": "vet-med-tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:stats": "next build && npx @next/bundle-analyzer build",
    "build:vercel": "pnpm dlx vercel build",
    "check": "biome check .",
    "check:unsafe": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:pull": "drizzle-kit pull --config=drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle.config.ts",
    "db:push:force": "drizzle-kit push --config=drizzle.config.ts --force",
    "db:seed": "tsx scripts/seed.ts",
    "db:seed:medications": "tsx scripts/seed-medications-fixed.ts",
    "db:seed:visual": "tsx scripts/seed-visual-test-data.ts",
    "db:migrate:prod": "drizzle-kit migrate --config=drizzle.config.ts",
    "db:migrate:staging": "DATABASE_URL=$STAGING_DATABASE_URL DATABASE_URL_UNPOOLED=$STAGING_DATABASE_URL_UNPOOLED drizzle-kit migrate --config=drizzle.config.ts",
    "db:rollback": "drizzle-kit rollback --config=drizzle.config.ts",
    "db:backup": "tsx scripts/backup-database.ts",
    "db:test:init": "tsx scripts/init-test-db.ts",
    "db:test:reset": "tsx scripts/init-test-db.ts --reset",
    "db:test:seed": "tsx scripts/init-test-db.ts --seed",
    "db:test:health": "tsx scripts/init-test-db.ts --health",
    "db:test:verify": "node scripts/verify-test-db.js",
    "deploy:staging": "pnpm build && vercel deploy --prebuilt",
    "deploy:production": "pnpm build && vercel deploy --prebuilt --prod",
    "db:studio": "drizzle-kit studio --config=drizzle.config.ts",
    "dev": "next dev",
    "dev:host": "next dev --hostname 0.0.0.0",
    "dev:turbo": "next dev --turbopack",
    "preview": "next build && next start",
    "preview:dev": "NODE_ENV=development next build && NODE_ENV=development next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint && next lint",
    "format": "biome format --write",
    "puppeteer:install": "puppeteer browsers install",
    "test": "vitest run",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage --config vitest.config.unit.ts",
    "test:integration": "vitest run tests/integration",
    "test:health": "tsx scripts/test-health-endpoints.ts",
    "test:e2e": "playwright test",
    "rate-limit:status": "node scripts/manage-rate-limits.js status",
    "rate-limit:clear": "node scripts/manage-rate-limits.js clear",
    "user:check": "bun scripts/fix-user-sync.ts --check-orphaned",
    "user:status": "bun scripts/fix-user-sync.ts --check-all",
    "user:fix": "bun scripts/fix-user-sync.ts",
    "user:clean-test": "bun scripts/fix-user-sync.ts --clean-test",
    "user:validate": "bun scripts/fix-user-sync.ts --validate-stack",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "percy exec -- playwright test tests/e2e/visual/",
    "test:visual:update": "percy exec -- playwright test tests/e2e/visual/ --update-snapshots",
    "test:all": "pnpm test:integration && pnpm test:e2e",
    "test:all:visual": "pnpm test:all && pnpm test:visual",
    "lighthouse": "pnpm build && lhci autorun",
    "lighthouse:ci": "lhci autorun --upload.target=temporary-public-storage",
    "perf:audit": "pnpm build && lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "bundle:size": "size-limit",
    "phylopic:curate": "bun scripts/fetch-phylopic-curated.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@neondatabase/serverless": "^1.0.1",
    "@next/env": "^15.4.6",
    "@radix-ui/react-accordion": "1.2.11",
