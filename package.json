		"db:seed:medications": "tsx scripts/seed-medications-fixed.ts",
		"db:studio": "drizzle-kit studio --config=drizzle.config.ts",
		"dev": "next dev",
		"dev:turbo": "next dev --turbopack",
		"preview": "next build && next start",
		"preview:dev": "NODE_ENV=development next build && NODE_ENV=development next start",
		"start": "next start",
		"typecheck": "tsc --noEmit",
		"lint": "biome lint && next lint",
		"format": "biome format --write",
		"puppeteer:install": "puppeteer browsers install",
		"test": "vitest run",
		"test:ui": "vitest --ui",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:integration": "vitest run tests/integration",
		"test:health": "tsx scripts/test-health-endpoints.ts",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"test:all": "pnpm test:integration && pnpm test:e2e",
		"phylopic:curate": "bun scripts/fetch-phylopic-curated.ts",
